package test;

public class PalindromeWithotMethod {

	public static void main(String[] args) {
		String s = "abcadd1232bcbad";
		
		for (int i = 0; i < s.length(); i++) 
		{
			String s1="";
			for (int j = i+1; j < s.length(); j++) 
			{
				for (int k = i; k <= j; k++) 
				{
					if(s.charAt(k)==s.charAt(k-1-i))
					{
						s1 = s1 +s.charAt(k);
					}
				}
			}
			System.out.println(s1);
		}
		

	}

}
