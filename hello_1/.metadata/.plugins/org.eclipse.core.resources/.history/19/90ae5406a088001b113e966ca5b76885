package test;

import java.util.Iterator;

public class Prime {

	public static void main(String[] args) {
		int[] a = {5,12,3,6,8,18};
		for (int i = 1; i <= a.length; i++) 
		{
			for (int j = i; j < a[i]; j++) {
				if(a[i]%j==0)
				{
					System.out.println(a[i]);
					break;
				}
				
				
			}
			
		}

	}

}
