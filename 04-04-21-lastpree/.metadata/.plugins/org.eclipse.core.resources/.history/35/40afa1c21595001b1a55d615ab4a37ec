package arraylist1;

public class MyArrayList {
	int def_cap=10;
	static int[] a = new int[10];
	static int index=0;
	
	public static void add(int k)
	{
		for (int i = 0; i < a.length; i++) 
		{
			a[index++]=k;
		}
		if(index==a.length)
		{
			int newLingth = a.length*2;
			int[] b = new int[newLingth];
			b[index++] = k;
			
		}
	}

}
