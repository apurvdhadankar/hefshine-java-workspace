package pree;

public class Q2_parttern {
	public static void main(String[] args) {
		for (int i = 0; i < 9; i++) 
		{
			for (int j = 0; j < 9; j++) 
			{
				if(j>3 && j<5 && i<2)
				{
					System.out.print("*");
				}
				else if(j>2 && j<6 && i>1 && i<4)
					System.out.print("*");
				else if(j>1 && j<7 && i>3 && i<6)
					System.out.print("*");
				else if(j>0 && j<8 && i>5 && i<8)
					System.out.print("*");
				else if(i>7)
				{
					System.out.print("*");
				}
				else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}

}
