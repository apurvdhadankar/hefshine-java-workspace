package arraylist1;

public class MyArrayList {
	int def_cap=10;
	int[] a = new int[10];
	int index=0;
	
	public  void add(int k)
	{
		for (int i = 0; i < a.length; i++) 
		{
			a[i]=k;
		}		
		
	}
	
	void print()
	{
		for (int i = 0; i < a.length; i++) 
		{
			System.out.print(a[i]+" ");
		}
	}

}
